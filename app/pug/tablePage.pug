extends template
block variables
	-var title = 'Природоохранные зоны'

block content
	.third-intro
		.overflow
		.container
			.third-intro__header
				h1.third-intro__title Заповедники России

	section.section
		.container
			.section__header
				h3.section__suptitle 
					span Сортировка
				h2.sectiou__suptitle Сортировать 
					span таблицу
			.section__sort
				details.details
					summary График
					form.details__form
						h3.detail__form__th Значение по OX 
						h3.detail__form__th Результат
						div.details__form__left
							input(type="radio" name="ox" value="0")
							span  Название
						div.details__form__right 
							input(type="checkbox" name="oy" value="0")
							span  Максимальная площадь
						div.details__form__left
							input(type="radio" name="ox" value="1")
							span  Расположение
						div.details__form__right 
							input(type="checkbox" name="oy" value="1")
							span  Новейшие
						div.details__form__right 
							input(type="checkbox" name="oy" value="2")
							span  Наибольшее количество проведённых туров
						button.details__form__button Построить
				details.details
					summary Фильтр
					form.details__form
						label.details__form__left(for="name") Название:
						input.details__form__input(type="text" name="name")
						label.details__form__left(for="place") Расположение:
						input.details__form__input(type="text" name="place")
						label.details__form__left(for="square") Площадь:
						input.details__form__input(type="number" name="squart")
						label.details__form__left(for="tours") Количество проведённых туров:
						input.details__form__input(type="number" name="tours")
						button.details__form__button Построить
				details.details
					summary Сортировка
					form.details__form
						label.details__form__left(for="first") Первый уровень
						select.details__form__select(name="first")
							option(value="0") Нет
							option(value="1") Название
							option(value="2") Расположение
							option(value="3") Площадь
							option(value="4") Дата образования
							option(value="5") Количество проведённых туров
						label.details__form__left(for="first-reverse") По убыванию?
						input.details__form__checkbox(type="checkbox" name="first-reverse")
						label.details__form__left(for="second") Первый уровень
						select.details__form__select(name="second")
							option(value="0") Нет
							option(value="1") Название
							option(value="2") Расположение
							option(value="3") Площадь
							option(value="4") Дата образования
							option(value="5") Количество проведённых туров
						label.details__form__left(for="second-reverse") По убыванию?
						input.details__form__checkbox(type="checkbox" name="second-reverse")
						button.details__form__button Построить


